{
    "name": "@navikt/nav-dekoratoren",
    "author": "NAVIKT",
    "version": "0.3.8",
    "description": "hode og fot navigasjon for nav.no",
    "homepage": "nav.no/person/nav-dekoratoren",
    "private": false,
    "main": "packagebuild/decorator.js",
    "module": "packagebuild/decorator.js",
    "types": "packagebuild/npm-package/index.d.ts",
    "scripts": {
        "start-craco": "craco start",
        "start": "cross-env DEVELOPMENT=true npm-run-all -p -r start-craco server",
        "start-s": "npm-run-all -p -r build-ssr start-ssr",
        "build": "craco build",
        "test": "craco test",
        "start-ssr": "node buildfolder/server.js",
        "build-ssr": "npm run build-client && npm run build-server",
        "build-ssr-dev": "npm run build-client-dev && npm run build-server-dev",
        "build-snap": "craco build && react-snapshot",
        "build-client": "cross-env NODE_ENV=production webpack -p --display-error-details --config webpack.client.config.js ",
        "build-server": "cross-env NODE_ENV=production webpack -p --display-error-details --config webpack.server.config.js",
        "build-client-dev": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.client.config.js ",
        "build-server-dev": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.server.config.js",
        "buildpackage": "npm run build-npm-file && npm run build-npm-d",
        "build-npm-file": "cross-env NODE_ENV=development webpack --display-error-details --config webpack.npm.build.config.js",
        "build-npm-d": "tsc --project tsconfig.typegen.json",
        "server": "nodemon src/server/server.js",
        "lint": "tslint ./src/**/*.ts*",
        "lint:fix": "tslint ./src/**/*.ts* --fix",
        "docker-build": "docker build . -t navikt/nav-dekoratoren",
        "docker-push": "docker push navikt/nav-dekoratoren",
        "docker-start": "docker run --rm -p 8088:8088 navikt/nav-dekoratoren",
        "deploy-dev": "kubectl apply -f nais-dev.json",
        "deploy-prod": "kubectl apply -f nais-prod.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/navikt/nav-dekoratoren.git"
    },
    "bugs": {
        "url": "https://github.com/navikt/nav-dekoratoren/issues"
